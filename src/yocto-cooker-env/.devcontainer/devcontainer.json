{
    "name": "Yocto|Cooker",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "features": {
        "ghcr.io/exactassembly/yocto-devcontainer-features/yocto-cooker" : {}
    },
    "containerEnv": {        
        "COOKER_WORKSPACE" : "/workspace",
        "COOKER_DL_CACHE" : "/var/lib/dl-cache",
        "COOKER_SSTATE_CACHE" : "/var/lib/dl-cache"
    },
	"workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "mads-hartmann.bash-ide-vscode",
                "EditorConfig.EditorConfig",
                "EugenWeins.bitbake"
            ]
        }
    }
}